               <font>
                  <Font name="System Bold Italic" size="18.0" />
               </font>
            </Label>
            <Button fx:id="finalizarButton" layoutX="534.0" layoutY="646.0" mnemonicParsing="false" onAction="#finalizarOnClick" prefHeight="69.0" prefWidth="133.0" text="Finalizar">
               <font>
                  <Font size="24.0" />
               </font></Button>
            <Label layoutX="414.0" layoutY="564.0" text="696 696 696">
               <font>
                  <Font name="System Italic" size="18.0" />
               </font>
            </Label>
            <Label layoutX="628.0" layoutY="564.0" text="Ayuda@airdnd.org">
               <font>
                  <Font name="System Italic" size="18.0" />
               </font>
            </Label>
